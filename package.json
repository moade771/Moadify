{
  "name": "moadify",
  "version": "0.8.0",
  "description": "A local offline music player with a Spotify like UI",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [
    "music",
    "player",
    "electron",
    "offline"
  ],
  "author": "moade77",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "discord-rpc": "^4.0.1",
    "electron-updater": "^6.6.2",
    "music-metadata": "^7.14.0",
    "uuid": "^9.0.1"
  },
  "build": {
    "appId": "com.moadify.app",
    "publish": [
      {
        "provider": "github",
        "owner": "moade771",
        "repo": "Moadify"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "icon.ico",
      "artifactName": "${productName}.Setup.${version}.${ext}"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moade771/Moadify"
  }
}